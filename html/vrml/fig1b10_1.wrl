  field SFFloat H
  field SFNode app
]"p_prism.wrl"

EXTERNPROTO SqSheet [
	field SFInt32 nx
	field SFInt32 ny
	field SFFloat d
  field SFColor color
]"p_sqs.wrl"

EXTERNPROTO Ray [
	eventIn	SFVec3f	set_Position
	eventIn	SFVec3f	set_Direction
	field SFVec3f Position
	field SFVec3f Direction
	field SFFloat StemRadius
 field SFFloat HeadPos
	field SFFloat HeadRadius
	field SFFloat HeadSize
	field SFColor HeadColor
	field	SFColor	StemColor
]"p_ray.wrl"

EXTERNPROTO Line [
  field SFVec3f startAt
  field SFVec3f endAt
  field SFColor color
]"p_line.wrl"

EXTERNPROTO Arc [
  field SFVec3f centerAt
  field SFFloat radius
  field SFFloat startAt
  field SFFloat angle
  field SFInt32 resolution
  field SFColor color
]"p_arc.wrl" 

EXTERNPROTO Label[
  field MFString text
  field MFString sub
  field SFVec3f position
  field SFFloat size
  field SFColor color
]"p_label.wrl"

