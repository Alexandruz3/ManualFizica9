#VRML V2.0 utf8

EXTERNPROTO Line [
  field SFVec3f startAt
  field SFVec3f endAt
  field SFColor color
]"p_line.wrl"

EXTERNPROTO SCLens [
  field SFVec3f position
  field SFFloat arrowAngle
  field SFFloat arrowLenght
  field SFColor color
  field SFFloat apperture
]"p_sclens.wrl"

EXTERNPROTO Arrow [
  field SFVec3f position
  field SFFloat arrowAngle
  field SFFloat arrowLenght
  field SFColor color
  field SFFloat lenght
]"p_arrow.wrl"

EXTERNPROTO Ray [
	eventIn	SFVec3f	set_Position
	eventIn	SFVec3f	set_Direction
	field SFVec3f Position
	field SFVec3f Direction
	field SFFloat StemRadius
 field SFFloat HeadPos
	field SFFloat HeadRadius
	field SFFloat HeadSize
	field SFColor HeadColor
	field	SFColor	StemColor
]"p_ray.wrl"

EXTERNPROTO Quota [
  field SFVec3f startAt
  field SFFloat arrowAngle
  field SFFloat arrowLenght
  field SFColor color
  field SFFloat lenght
]"p_quota.wrl"

EXTERNPROTO Label[
  field MFString text
  field MFString sub
  field SFVec3f position
  field SFFloat size
  field SFColor color
]"p_label.wrl"

EXTERNPROTO Arc [
  field SFVec3f centerAt
  field SFFloat radius
  field SFFloat startAt
  field SFFloat angle
  field SFInt32 resolution
  field SFColor color
]"p_arc.wrl" 


